#include <stdio.h>
#include <math.h>

int main()
{
  float a,b,c,D,x1,x2;
    printf("Введите первое значение: ");
    scanf("%f", &a);
    printf("Введите второе значение: ");
    scanf("%f", &b);
    printf("Введите третье значение: ");
    scanf("%f", &c);
    D=b*b-4*a*c;
    printf("D = %f\n",D);
    if (D<0){
        printf("Корней нет");
    } else if (D=0){
        x1=-b+(sqrt(D))/(2*a);
        printf("Корень x1 = %f",x1);
    } else if (D>0){
        x1=(-b+sqrt(D))/(2*a);
        x2=(-b-sqrt(D))/(2*a);
        printf("Корень x1 = %f\n",x1);
        printf("Корень x2 = %f",x2);
    }
    return 0;
}

